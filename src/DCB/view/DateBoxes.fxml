<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DCB.view.DateBoxesController">
   <children>
      <GridPane layoutX="68.0" layoutY="64.0" prefHeight="272.0" prefWidth="464.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label alignment="CENTER" prefHeight="21.0" prefWidth="243.0" text="Month" />
            <Label alignment="CENTER" prefHeight="21.0" prefWidth="240.0" text="Day" GridPane.rowIndex="1" />
            <Label alignment="CENTER" prefHeight="21.0" prefWidth="234.0" text="Year" GridPane.rowIndex="2" />
            <ComboBox fx:id="Month" onAction="#handleMonths" prefHeight="31.0" prefWidth="248.0" GridPane.columnIndex="1" />
            <ComboBox fx:id="Day" prefHeight="31.0" prefWidth="245.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <ComboBox fx:id="Year" onAction="#handleYears" prefHeight="31.0" prefWidth="259.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
